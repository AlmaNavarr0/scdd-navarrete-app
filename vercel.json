{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        },
        {
            "src": "vercel.json", 
            "use": "vercel-sapper"
        }
    ], 
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}