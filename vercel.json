{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        },
        {
            "src": "package.json", 
            "use": "vercel-sapper"
        }
    ], 
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "api/index.js": {
          "memory": 1024,
          "maxDuration": 90
        }
      }
}