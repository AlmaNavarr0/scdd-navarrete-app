            </div>
            <small class="d-inline-block mb-2 text-secondary fw-bold"><i class="small text-light text-muted fa-regular fa-clock"></i> Próximas vigencias </small>
                <h3 class="fw-semibold" style="font-family: 'Quicksand', sans-serif; color: rgb(15, 85, 85);">Vigencias en tarjetón vehícular  <i
                    class="float-end fa-solid fa-thumbtack" style="color: rgb(15, 85, 85);"></i></h3>
            <div class="mb-1" style="font-weight: bolder; font-family: 'Raleway', sans-serif;">
              <%= fecha%>
            </div>
            <% if(tarjeton.length==0){
              var tar_feli = 'No hay vigencias próximas, genial, lindo día.'
            } else { tar_feli = 'Los siguiente tarjetones vehículares estan por vencer pronto:'}
            %>
            <p style="font-weight: bold; font-family: 'Raleway', sans-serif;" class="mb-auto pt-2"> <%= tar_feli%>
            </p>
            <ol class="list-group list-group-numbered my-2 mb-2" style="font-weight: bold;">
              <%tarjeton.forEach((tarjeton)=>{%>
              <li class="list-group-item d-flex justify-content-between align-items-start mb-2"
                style="background-color: rgba(81, 123, 143, 0.199);">
                <% if(tarjeton.foto_trabajador.includes("undefined") || (tarjeton.foto_trabajador=="" || tarjeton.foto_trabajador[tarjeton.foto_trabajador.length-1]=='-')){ 
                  var foto_tar='fondo_full.jpg' ; 
                  } else { foto_tar=tarjeton.foto_trabajador; } %>
                  <a href="/panel/administracion/ir/<%=tarjeton.email_trabajador%>" style="cursor: pointer;"> <img src="http://localhost:3000/trabajadores/<%= foto_tar%>" alt="mdo" width="32" height="32"
                  class="mx-2 d-flex rounded-circle"> </a>
                <div class="ms-2 me-auto">
                  <div class=""> <a href="/panel/administracion/ir/<%=tarjeton.email_trabajador%>" style="cursor: pointer; text-decoration: none;"> <strong style="font-weight: 650" class="text-muted"> <%= tarjeton.nombre_trabajador%></strong></a></div>
                  <small style="font-weight: 500; "><%= tarjeton.dia_tarjeton%> de <%= tarjeton.mes_tarjeton%>, <%= tarjeton.anyo_tarjeton%> </small>
                </div>
                <span class="badge rounded-pill"
                  style="background-image: linear-gradient(-60deg, rgb(204, 51, 51) 0%, rgb(139, 55, 36) 100%);"><%= tarjeton.dia_tarjeton%> / <%= tarjeton.mes_tarjeton%> / <%= tarjeton.anyo_tarjeton%></span>
              </li>
              <%});%>
            </ol>
            <a href="/panel/administracion/listado-trabajadores" class="pt-0"
              style="font-weight: bold; text-decoration: none; font-family: 'Raleway', sans-serif; color: teal;">Ver lista
              completa</a>
          </div>
          <div class="col-auto d-none d-lg-block">
            <svg class="bd-placeholder-img" width="200" height="250" xmlns="http://www.w3.org/2000/svg" role="img"
              aria-label="Placeholder: Thumbnail" preserveAspectRatio="xMidYMid slice" focusable="false">
              <title>Placeholder</title>
              <rect width="100%" height="100%" fill="#55595c"></rect><text x="50%" y="50%" fill="#eceeef"
                dy=".3em">Thumbnail</text>
            </svg>
          </div>
        </div>
      </div>
    </div>
</div>
</div>
  </section>

